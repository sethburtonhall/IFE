  }

form select {
  width: 100% !important;
}*/

/* ------------------------------------------------------------------------ Footer */

/*#footer {
  background: url('../images/footer-bg.jpg') no-repeat 99% 50%;
}*/

/* ------------------------------------------------------------------------ Media Queries */

/* Smartphones */
@media (max-width: 480px) {

#site-description {
  color: $darkgray;
  @include adjust-font-size-to(13px);
  letter-spacing: .1em;
}

#hgroup {
  padding: 10px 0px 28px 0;
  position: relative;
}

.home .hero-unit {
  padding: 10px 10px;
  text-align: center;
}

.home-well {
  padding: 87px 0px 0px 0px;
}

.well, .investment-well {
  margin-bottom: $basespace !important;
}

.step-one-well,
.step-two-well,
.step-three-well,
.step-four-well,
.home2-well,
.home3-well {
  margin-bottom: $basespace !important;
}

.spotlight-hr {
  border-top: 1px solid $darkgray;
  margin: 18px 0px !important;
}

}

/* Tablet and Mobile */
@media (max-width: 979px) {

.home-icon{
  display: none;
}

.navbar .social-icons > li {
  margin: 5px 0 0px 12px;
}

.step-three-well {
  padding: 67px 14px 19px 19px;
}

.well::before {
  @include adjust-font-size-to(18px);
}

}

/* Mobile to Tablet */
@media only screen and (max-width: 767px) {

#footer .container {
  padding-left: $basespace;
  padding-right: $basespace;
}

}

/* Landscape Tablets */
@media (min-width: 768px) and (max-width: 979px) {


}

/* Desktop */
@media (min-width: 980px) {

}

